{
  "name": "mkdocs-material",
  "version": "8.1.8",
  "description": "A Material Design theme for MkDocs",
  "keywords": [
    "mkdocs",
    "documentation",
    "theme"
  ],
  "homepage": "https://squidfunk.github.io/mkdocs-material/",
  "bugs": {
    "url": "https://github.com/squidfunk/mkdocs-material/issues",
    "email": "martin.donath@squidfunk.com"
  },
  "license": "MIT",
  "private": true,
  "author": {
    "name": "Martin Donath",
    "email": "martin.donath@squidfunk.com"
  },
  "contributors": [],
  "repository": {
    "type": "git",
    "url": "https://github.com/squidfunk/mkdocs-material.git"
  },
  "scripts": {
    "build": "rimraf material && ts-node -T tools/build --optimize",
    "build:dirty": "ts-node -T tools/build --dirty",
    "clean": "rimraf material",
    "check": "run-p check:*",
    "check:build": "tsc --noEmit",
    "check:style": "run-p check:style:*",
    "check:style:scss": "stylelint \"src/**/*.scss\"",
    "check:style:ts": "eslint --cache \"src/**/*.ts\"",
    "start": "ts-node -T tools/build --verbose --dirty --watch",
    "upgrade": "run-s upgrade:*",
    "upgrade:bump": "ncu --upgrade --filterVersion \"/^\\^/\"",
    "upgrade:install": "npm install"
  },
  "dependencies": {
    "array-flat-polyfill": "^1.0.1",
    "clipboard": "^2.0.8",
    "escape-html": "^1.0.3",
    "focus-visible": "^5.2.0",
    "fuzzaldrin-plus": "^0.6.0",
    "lunr": "^2.3.9",
    "lunr-languages": "^1.9.0",
    "resize-observer-polyfill": "^1.5.1",
    "rxjs": "^7.5.2",
    "unfetch": "^4.2.0",
    "url-polyfill": "^1.1.12"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.15.4",
    "@mdi/svg": "^6.5.95",
    "@primer/octicons": "^16.2.0",
    "@types/clipboard": "^2.0.7",
    "@types/escape-html": "1.0.1",
    "@types/fuzzaldrin-plus": "^0.6.2",
    "@types/html-minifier": "^4.0.2",
    "@types/lunr": "^2.3.4",
    "@types/node": "^17.0.10",
    "@types/resize-observer-browser": "^0.1.6",
    "@types/sass": "^1.43.1",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "autoprefixer": "^10.4.2",
    "chokidar": "^3.5.3",
    "cssnano": "^5.0.16",
    "esbuild": "^0.14.13",
    "eslint": "^8.7.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^37.6.3",
    "eslint-plugin-no-null": "^1.0.2",
    "github-types": "^1.0.0",
    "gitlab": "^14.2.2",
    "html-minifier": "^4.0.0",
    "material-design-color": "^2.3.2",
    "material-shadows": "^3.0.1",
    "npm-check-updates": "^12.2.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "postcss-dir-pseudo-class": "^6.0.3",
    "postcss-inline-svg": "^5.0.0",
    "postcss-logical": "^5.0.3",
    "preact": "^10.6.4",
    "rimraf": "^3.0.2",
    "sass": "^1.49.0",
    "stylelint": "^14.3.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended": "^6.0.0",
    "stylelint-config-standard-scss": "^3.0.0",
    "stylelint-scss": "^4.1.0",
    "svgo": "2.3.1",
    "tiny-glob": "^0.2.9",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.5"
  },
  "engines": {
    "node": ">= 14"
  }
}
